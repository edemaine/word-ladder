{
  "name": "word-ladder",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "DEBUG=vite:* vite build",
    "serve": "vite preview",
    "test": "vitest",
    "lint": "eslint **/*.civet",
    "deploy": "npm run build && gh-pages -d dist",
    "prepare": "npm run wordlist && npm run convert-civet",
    "wordlist": "curl --create-dirs -o data/wordlist.ecprice.txt https://www.mit.edu/~ecprice/wordlist.10000 && curl --create-dirs -o data/wordlist.dwyl.txt https://cdn.jsdelivr.net/gh/dwyl/english-words/words_alpha.txt",
    "convert-civet": "civet --js -c src/worker.civet -o src/worker.js && civet --js -c src/ladder.civet -o src/ladder.js"
  },
  "license": "MIT",
  "devDependencies": {
    "@danielx/civet": "0.7.27",
    "@eslint/js": "^9.9.1",
    "eslint": "^9.9.1",
    "eslint-plugin-civet": "^0.0.5",
    "eslint-plugin-solid": "^0.14.2",
    "gh-pages": "^6.1.1",
    "sass": "^1.77.8",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-solid": "^2.10.1",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "solid-js": "^1.8.22"
  },
  "packageManager": "pnpm@9.1.1+sha256.9551e803dcb7a1839fdf5416153a844060c7bce013218ce823410532504ac10b"
}
